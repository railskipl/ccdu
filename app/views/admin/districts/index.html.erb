<h1>Listing Districts</h1>

<table>
  <tr>
    <th>District name</th>
    <th>Zone</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_districts.each do |admin_district| %>
  <tr>
    <td><%= admin_district.district_name rescue nil%></td>
    <td><%= admin_district.zone_id rescue nil%></td>
    <td><%= link_to 'Show', admin_district %></td>
    <td><%= link_to 'Edit', edit_admin_district_path(admin_district) %></td>
    <td><%= link_to 'Destroy', admin_district, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

