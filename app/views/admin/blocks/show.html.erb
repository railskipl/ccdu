<div class="main">

    <div class="container">

      <div class="row">
        
    <div class="span12">          
          
          <div class="widget stacked ">
            
            <div class="widget-header">
              <i class="icon-pencil"></i>
              <h3>Show Block</h3>
          </div> <!-- /widget-header -->
        <div class="widget-content">

          <div class="row-fluid">
                  <div class="span12">
                  
                    <table width="100%" border="0" cellspacing="0" cellpadding="0">
              
                      <tr>
                        <td>&nbsp;</td>
                        <td>
                          <%= link_to "Back", admin_blocks_path, :class => "btn"%>
                          <%= link_to "Edit Block", edit_admin_block_path(@admin_block), :class=>"btn", :type=>"reset"%>
                        </td>
                        <td>&nbsp;</td>
                        <td>&nbsp;</td>
                        <td>&nbsp;</td>
                        <td>&nbsp;</td>
                      </tr>
                    </table>
                  </div>
                </div>
                </br>
        
        
        <section id="tables">
          
          
          
          
          
          <table class="table table-bordered table-striped">
                   
                    <tbody>
                      <tr>
                        <td>Block name</td>
                        <td><%= @admin_block.block_name rescue nil%></td>
                        
                      </tr>
                      <tr>
                        <td>Code</td>
                        <td><%= @admin_block.code rescue nil%></td>
                        
                      </tr>
                      <tr>
                        <td>District</td>
                        <td><%= @admin_block.district.district_name rescue nil%></td>
                        
                      </tr>
                      <tr>
                        <td>Is Laboratory Present ?</td>
                        <td><%= @admin_block.laboratory_present rescue nil%></td>
                        
                      </tr>
                      <tr>
                        <td>Laboratory Name</td>
                        <td><%= @admin_block.lab_name rescue nil%></td>
                        
                      </tr>


                        
                 </tbody>
            </table>
                  
                  
                 

            
            
                  
          </section>
                
            
          
          
          
        </div> <!-- /widget-content -->
          
      </div> <!-- /widget -->
          
      </div>
        
      </div> <!-- /row -->

    </div> <!-- /container -->
    
</div> <!-- /main -->







